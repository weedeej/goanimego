{
  "expo": {
    "name": "goanimego",
    "slug": "goanimego",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/iconMain.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "backgroundColor": "#1c1b1f",
      "translucent": false,
      "barStyle": "dark-content"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/iconMain.png"
      }
    },
    "web": {
      "favicon": "src/assets/iconMain.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ]
  }
}
